<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="regression suite" verbose="1" >
    <groups>
        <run>
            <include name="regression"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.tutorialsninja.customlistners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="regression Test" >
        <classes>
            <class name="com.tutorialsninja.testsuite.AccountLoginPageTest"/>
            <class name="com.tutorialsninja.testsuite.AccountRegisterPageTest"/>
            <class name="com.tutorialsninja.testsuite.DesktopTest"/>
            <class name="com.tutorialsninja.testsuite.LaptopsAndNoteBooksTest"/>
            <class name="com.tutorialsninja.testsuite.TopmenuTest"/>
        </classes>
    </test>
</suite>